<div class="container-fluid head_section">
	<div class="row">
		<div class="col-xs-12 text-center">
			<h5 class="title">Results</h5>
		</div>
	</div>
</div>

<div class="container-fluid">

	<div class="row">
		<div class="col-sm-8 col-sm-push-2 top_section text-center">
			
			<h4>Your final score was <%= @test.score %>/3. 

				<% if @test.score < 3 %>
					Practice makes perfect!

				<% else %>
					That's perfect! Good job!
				<% end %>
			</h4>
				<br />
				<br />
				<%= link_to "Finish", tests_path, class: "btn btn-success text-center" %> 
		</div>
	</div>
	
</div>
